{
  "compilerOptions": {
    "target": "ES2024",
    "module": "Node16",
    "moduleResolution": "node16",    
    "rootDir": "./",
    "baseUrl": "./",
    "outDir": "./dist",
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "paths": {
        "@models/*": ["./@cds-models/*/index.ts"]
    }
  },
  "include": ["srv"],
  "exclude": ["node_modules","@cds-models"],
  "skipLibCheck": true
}
